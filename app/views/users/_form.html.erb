      <%= bootstrap_form_for(@user) do |f| %>

        <%= render 'shared/error_messages' %>

        <%#= f.label :token %>
        <%#= f.hidden_field :invitation_token %>

        <%#= f.label :invitation_id %>
        <%#= f.hidden_field :invitation_id %>
        
        <%= f.text_field :name,label:"姓名" %>

        <%= f.text_field :alipayid,label:"支付宝账号" %>

        <%= f.text_field :wechatid,label:"微信账号" %>

        <%= f.hidden_field :email,hide_label: :true,placeholder:"电子邮件" %>

        <%= f.password_field :password, label:"输入密码，验证身份" %>

        <%= f.password_field :password_confirmation, label:"确认密码" %>
        
        <%= f.primary "确认" %>

      <% end %>

