<% if uiships %>
  <ul>
    <% uiships.each do |uiship| %>
    <div class="product-room">
      <li class="product-item">
        <div class="product-url">
          <%= link_to uiship.idea.name,'#' %>
          <p><%= uiship.idea.intro%></p>
        </div>
          <div class="product-meta">
        <%= link_to '详情', uiship.idea %>
        </div>
      </li>
    </div>
    <% end %>
  </ul>
<% end %>


