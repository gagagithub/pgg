  <div class="container-fluid">  

    <div class= "page-keypart">
      <h1>登陆</h1>
    </div>

    <div class= "page-content">

        <%= bootstrap_form_tag url:'/sessions' do |f| %>

        <%= f.hidden_field :myweixincode, value: params[:weixincode] %>
        
        <%= f.text_field :name,label:"用户名" %>

        <%= f.password_field :password, label:"密码" %>

        <%= f.primary " 登陆 " %>
      <% end %>

    </div>

  </div>



